<p id="notice"><%= notice %></p>

<h1>Listing Modules</h1>

<table>
  <thead>
    <tr>
      <th>UE</th>
      <th>Titre</th>
      <th>Objectif</th>
      <th>Contenu</th>
      <th>Type</th>
      <th>Découpage</th>
      <th>Éval.1</th>
      <th>Éval.2</th>
      <th>Coefficient</th>
      <th>Bibliographie</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cours.each do |cour| %>
      <tr>
        <td><%= cour.ue.titre %></td>
        <td><%= cour.titre %></td>
        <td><%= cour.objectif %></td>
        <td><%= cour.contenu %></td>
        <td><%= cour.genre %></td>
        <td><%= cour.decoupage %></td>
        <td><%= cour.evaluation1 %></td>
        <td><%= cour.evaluation2 %></td>
        <td><%= cour.coeff %></td>
        <td><%= cour.bibliographie %></td>
        <td><%= link_to 'Show', cour %></td>
        <td><%= link_to 'Edit', edit_cour_path(cour) %></td>
        <td><%= link_to 'Destroy', cour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cour', new_cour_path %>
