<h1>Listing Ues</h1>

<table>
  <thead>
    <tr>
      <th>Semestre</th>
      <th>Titre</th>
      <th>Objectif</th>
      <th>Lieu</th>
      <th>Pr√©requis</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ues.each do |ue| %>
      <tr>
        <td><%= ue.semestre.numero %></td>
        <td><%= ue.titre %></td>
        <td><%= ue.objectif %></td>
        <td><%= ue.prerequis %></td>
        <td><%= ue.lieu %></td>
